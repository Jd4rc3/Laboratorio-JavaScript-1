{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "exclude": [
      "node_modules",
      ".git",
      ".DS_Store",
      "stylesheets",
      "views",
      "images"
    ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "templates/default",
    "encondig": "utf8",
    "destination": "./docs",
    "recurse": true
  }
}